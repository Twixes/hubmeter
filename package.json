        "eslint-plugin-simple-import-sort": "^7.0.0",
        "focus-visible": "^5.2.0",
        "framer-motion": "^3.10.3",
        "posthog-js": "^1.9.5",
        "react": "^17.0.1",
        "react-cookie": "^4.0.3",
        "react-dom": "^17.0.1",
        "react-error-boundary": "^3.1.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3",
