        "focus-visible": "^5.2.0",
        "framer-motion": "^4.1.17",
        "posthog-js": "^1.11.4",
        "react": "^17.0.2",
        "react-cookie": "^4.0.3",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.3",
